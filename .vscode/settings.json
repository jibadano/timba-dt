// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{

     "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/src/**/*.map":true,
        "**/src/**/*.js":true,
        "**/.gitignore":true
    },
    "editor.scrollBeyondLastLine": false,
    "typescript.check.workspaceVersion": false
}